PROJECTS = DUnit.exe Demo.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DUnit.exe: Test\DUnit.dpr
  $(DCC)

Demo.exe: Demo.dpr
  $(DCC)


